{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt|favicon\\.ico|assets|.*\\.(png|jpg|jpeg|gif|svg|webp|ico|css|js|json|xml|txt)).*)",
      "destination": "/index.html"
    }
  ]
}
